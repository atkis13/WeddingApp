            <Font name="System Bold" size="18.0" />
         </font></TextField>
      <Button fx:id="btn_add" layoutX="128.0" layoutY="487.0" mnemonicParsing="false" onAction="#AddSum" prefHeight="45.0" prefWidth="94.0" text="Insert" textOverrun="CENTER_ELLIPSIS" />
